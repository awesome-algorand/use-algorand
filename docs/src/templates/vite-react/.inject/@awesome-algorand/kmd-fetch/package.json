{
  "name": "@awesome-algorand/kmd-fetch",
  "version": "3.25.0",
  "description": "Fetch client for KMD",
  "keywords": [
    "algorand",
    "kmd",
    "fetch",
    "rpc"
  ],
  "type": "module",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "dev": "vitest --ui --coverage",
    "test": "vitest",
    "lint": "eslint src",
    "docs": "typedoc",
    "lint:fix": "eslint src --fix",
    "coverage": "vitest run --coverage",
    "clean": "rimraf src/*.ts lib",
    "build": "tsc && tsc-alias -p tsconfig.json -f"
  },
  "author": "Awesome Algorand",
  "license": "Unlicense",
  "dependencies": {
    "@awesome-algorand/core-fetch": "^1.0.0"
  }
}