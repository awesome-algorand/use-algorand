{
	"name": "@awesome-algorand/svelte-query",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@awesome-algorand/algod-fetch": "^3.25.0",
		"@awesome-algorand/indexer-fetch": "^3.5.0",
		"@awesome-algorand/kmd-fetch": "^3.25.0",
		"@awesome-algorand/query-core": "^1.0.0",
		"@tanstack/svelte-query": "^5.51.16",
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@awesome-algorand/algod-fetch": "^3.25.0",
		"@awesome-algorand/indexer-fetch": "^3.5.0",
		"@awesome-algorand/kmd-fetch": "^3.25.0",
		"@awesome-algorand/query-core": "^1.0.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tanstack/query-devtools": "^5.51.16",
		"@tanstack/svelte-query": "^5.51.16",
		"@tanstack/svelte-query-devtools": "^5.51.16",
		"@types/eslint": "^9.6.0",
		"@types/markdown-it": "^14.1.2",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"markdown-it": "^14.1.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.11",
		"vitest": "^2.0.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
